        <script lang="javascript" type="text/javascript">
            function OpenModual(page_url, dlgw, dlgh) {
                var ReturnedValue = showModalDialog(page_url, "Passed String",
                    "dialogWidth:" + dlgw + "px; dialogHeight:" + dlgh + "px; status:no; center:yes");
            }
            //  //window.document.title = "Amogh Piping";

            document.addEventListener('click', function (event) {
                if (!OpenSliderId.contains(event.target))
                    if (RadPanelSlider.contains(event.target) && !CloseSliderID.contains(event.target)) {
                        document.getElementById("RadPanelSlider").style.width = "250px";
                    }
                    else {
                        document.getElementById("RadPanelSlider").style.width = "0";
                    }
            });
            function OpenSliderClick() {
                document.getElementById("RadPanelSlider").style.width = "250px";
            }

            function CloseSliderClick() {
                document.getElementById("RadPanelSlider").style.width = "0";
            }
            $(document).on("mouseover", function (e) {
                if (e.pageX <= 0) {
                    document.getElementById("RadPanelSlider").style.width = "250px";
                }
                else if (e.pageX > 250) {
                    document.getElementById("RadPanelSlider").style.width = "0";
                }
            });
            window.onkeyup = function (event) {
                if (event.keyCode == 27) {
                    document.getElementById("PanelDiv").style.visibility = "hidden";
                }
            }
        </script>
        <script type="text/javascript">
            function Rebind_Parent(sender, args) {
                var temp = new Array();
                var str = document.getElementById('RadGridListHiddenField').value;
                var str2 = document.getElementById('AspGridListHiddenField').value;
                var str3 = document.getElementById('ButtonListHiddenField').value;

                temp = str.split(",");
                if (str.length > 0) {
                    for (var i = 0; i < temp.length; i++) {
                        var masterTable = $find(temp[i]).get_masterTableView();
                        masterTable.rebind();
                    }
                }

                if (str2.length > 0) {
                    document.location.reload();
                }
                temp1 = str3.split(",");
                if (str3.length > 0) {
                    for (var i = 0; i < temp1.length; i++) {
                        var buttonClick = document.getElementById(temp1[i]).click();
                    }
                }

            }

        </script>
