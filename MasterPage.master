            #grad {
                background-color: #c1d7fc;
                background: -webkit-linear-gradient(left, #96baf4, #dfeafb); /* For Safari 5.1 to 6.0 */
                background: -o-linear-gradient(right, #96baf4, #dfeafb); /* For Opera 11.1 to 12.0 */
                background: -moz-linear-gradient(right, #96baf4, #dfeafb); /* For Firefox 3.6 to 15 */
                background: linear-gradient(to right, #96baf4, #dfeafb); /* Standard syntax */
            }

            .RadWindowMange {
                z-index: 8000;
            }

            .sidenav {
                height: 100%;
                width: 0;
                position: fixed;
                z-index: 8000;
                top: 0;
                left: 0;
                overflow-x: hidden;
                transition: 0.5s;
                top: 90px;
            }

                .sidenav a {
                    text-decoration: none;
                    color: #FF7133;
                    display: block;
                    transition: 0.3s;
                }

                    .sidenav a:hover {
                        color: #FFC300;
                    }

                .sidenav .closebtn {
                    position: absolute;
                    top: 0;
                    right: 15px;
                    font-size: 36px;
                    margin-left: 50px;
                }

            @media screen and (max-height: 450px) {
                .sidenav {
                    padding-top: 15px;
                }

                    .sidenav a {
                        font-size: 18px;
                    }
            }

            #LoadingBackground {
                position: absolute;
                top: 0px;
                bottom: 0px;
                left: 0px;
                right: 0px;
                overflow: hidden;
                background-color: #000;
                filter: alpha(opacity=50);
                opacity: 0.5;
                z-index: 1000;
            }

            .MessagePanelDiv {
                position: fixed;
                top: 50%;
                left: 50%;
                transform: translate(-50%, -50%);
                width: 35%;
                z-index: 99;
                font-size: 15px;
